import org.junit.Test;
import static org.junit.Assert.*;

public class WordSuggesterTest {

    @Test
    public void testGetValidKeys() throws Exception {
        CoreDictionary bestWordSelector = new CoreDictionary();
        WordSuggester sc = new WordSuggester("apple*d", bestWordSelector.getDictionary());
        sc.getValidKeys();

    }

    @Test
    public void testGetMaxScoreWords() throws Exception {
        CoreDictionary bestWordSelector = new CoreDictionary();
        WordSuggester sc = new WordSuggester("apple*d", bestWordSelector.getDictionary());
        sc.getMaxScoreWords();
    }
}
